{% extends 'base.html.twig' %}

{% block title %}Hello ContactController!{% endblock %}

{% block body %}
{{ form_start(form) }}
<div class="row">
{{ form_row(form.service) }}</div>
<div class="row">
    <div class="col-6">
{{ form_row(form.name) }}
    </div>
    <div class="col-6">
{{ form_row(form.email) }}
    </div>
</div>
{{ form_rest(form) }}
{{ form_end(form) }}
{% endblock %}
